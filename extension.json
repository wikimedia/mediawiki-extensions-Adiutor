{
	"name": "Adiutor",
	"author": "Vikipolimer",
	"url": "https://www.mediawiki.org/wiki/Extension:Adiutor",
	"descriptionmsg": "adiutor-desc",
	"version": "0.0.1",
	"license-name": "GPL-2.0-or-later",
	"type": "other",
	"config": {
		"AdiutorEnable": {
			"description": "Enable the adiutor features",
			"value": true
		}
	},
	"MessagesDirs": {
		"Adiutor": [
			"i18n"
		]
	},
	"requires": {
		"MediaWiki": ">=1.41"
	},
	"ExtensionMessagesFiles": {
		"AdiutorAlias": "Adiutor.i18n.alias.php"
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\Adiutor\\": "src/"
	},
	"DefaultUserOptions": {
		"adiutor-enable": 0,
		"adiutor-csd-enable": 1,
		"adiutor-rpp-enable": 1,
		"adiutor-rpm-enable": 1,
		"adiutor-prod-enable": 1,
		"adiutor-tag-enable": 1
	},
	"SpecialPages": {
		"AdiutorSettings": {
			"class": "MediaWiki\\Extension\\Adiutor\\AdiutorSettings"
		}
	},
	"Hooks": {
		"BeforePageDisplay": "pageDisplay",
		"GetPreferences": "preferences",
		"GetBetaFeaturePreferences": "betaFeature",
		"SaveUserOptions": "preferences",
		"ListDefinedTags": "listDefinedTags",
		"ChangeTagsListActive": "changeTagsListActive",
		"ChangeTagsAllowedAdd": "changeTagsAllowedAdd",
		"PageSaveComplete": "pageSaveComplete"
	},
	"HookHandlers": {
		"preferences": {
			"class": "MediaWiki\\Extension\\Adiutor\\HookHandler\\PreferencesHandler",
			"services": [
				"PermissionManager",
				"UserOptionsLookup",
				"UserGroupManager"
			]
		},
		"betaFeature": {
			"class": "MediaWiki\\Extension\\Adiutor\\HookHandler\\BetaFeaturePreferencesHandler",
			"services": [
				"MainConfig",
				"PermissionManager"
			]
		},
		"pageDisplay": {
			"class": "MediaWiki\\Extension\\Adiutor\\HookHandler\\PageDisplayHandler",
			"services": [
				"PermissionManager",
				"UserOptionsLookup",
				"RevisionLookup",
				"MainWANObjectCache"
			]
		},
		"listDefinedTags": {
			"class": "MediaWiki\\Extension\\Adiutor\\HookHandler\\ChangeTagsHandler",
			"services": [
				"MainConfig"
			]
		},
		"changeTagsListActive": {
			"class": "MediaWiki\\Extension\\Adiutor\\HookHandler\\ChangeTagsHandler",
			"services": [
				"MainConfig"
			]
		},
		"changeTagsAllowedAdd": {
			"class": "MediaWiki\\Extension\\Adiutor\\HookHandler\\ChangeTagsHandler",
			"services": [
				"MainConfig"
			]
		},
		"pageSaveComplete": {
			"class": "MediaWiki\\Extension\\Adiutor\\HookHandler\\PageSaveCompleteHandler",
			"services": [
				"MainConfig"
			]
		}
	},
	"ExtensionFunctions": [
		"MediaWiki\\Extension\\Adiutor\\Adiutor::onExtensionLoad"
	],
	"ResourceFileModulePaths": {
		"localBasePath": "resources",
		"remoteExtPath": "Adiutor/resources"
	},
	"ResourceModules": {
		"ext.adiutor": {
			"packageFiles": [
				"ext.adiutor/init.js",
				"ext.adiutor/components/deletionPropose.vue",
				"ext.adiutor/components/createSpeedyDeletion.vue",
				"ext.adiutor/components/createSpeedyDeletionOptions.vue",
				"ext.adiutor/components/articleTagging.vue",
				"ext.adiutor/components/articleTaggingOptions.vue",
				"ext.adiutor/components/requestPageMove.vue",
