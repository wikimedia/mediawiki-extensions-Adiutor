				"ext.adiutor/components/adiutorSettings.vue",
				"ext.adiutor/icons.json",
				{
					"name": "ext.adiutor/icons.json",
					"callback": "MediaWiki\\ResourceLoader\\CodexModule::getIcons",
					"callbackParam": [
						"cdxIconAdd",
						"cdxIconClose",
						"cdxIconSearch"
					]
				}
			],
			"dependencies": [
